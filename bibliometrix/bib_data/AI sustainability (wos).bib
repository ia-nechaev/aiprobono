@article{ WOS:000524899601052,
Author = {Choi, Eunjeong and Cho, Soohwan and Kim, Dong Keun},
Title = {{Power Demand Forecasting Using Long Short-Term Memory (LSTM)
   Deep-Learning Model for Monitoring Energy Sustainability}},
Journal = {{SUSTAINABILITY}},
