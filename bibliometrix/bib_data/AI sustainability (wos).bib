@article{ WOS:000617116700052,
Author = {El-Emary, Ibrahim M. M. and Abdullah, Hassan Abdullah Alqarni and
   Alqahtani, Mohmmed and Alqahtani, Faisal Ahmed},
Title = {{Applications of Artificial Intelligence in Clean Sustainable Energy
   System Support}},
