@article{ WOS:000703907200008,
Author = {Ates, Kubra Tumay and Sahin, Cenk and Kuvvetli, Yusuf and Kuren, Bulent
   A. and Uysal, Aykut},
Title = {{Sustainable production in cement via artificial intelligence based
   decision support system: Case study}},
