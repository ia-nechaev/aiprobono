@article{ WOS:000689988100001,
Author = {Camarena, Stephanie},
Title = {{Engaging with Artificial Intelligence (AI) with a Bottom-Up Approach for
   the Purpose of Sustainability: Victorian Farmers Market Association,
   Melbourne Australia}},
