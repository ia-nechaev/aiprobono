@ARTICLE{Wu2019,
author={Wu, M. and Wei, Y. and Lam, P.T.I. and Liu, F. and Li, Y.},
title={Is urban development ecologically sustainable? Ecological footprint analysis and prediction based on a modified artificial neural network model: A case study of Tianjin in China},
journal={Journal of Cleaner Production},
